    "    catalog_df = pd.read_csv(catalog_file,encoding = \"latin-1\")\n",
    "    for index, row in catalog_df.iterrows():\n",
    "        code = row['CLAVE']\n",
    "        value = row['DESCRIPCIÃN']\n",
    "        covid_df[columnName] = covid_df[columnName].replace(code, value)\n",
    "    print(covid_df[columnName].head())\n",
    "\n",
    "#preview original data\n",
    "covid_df.head()"
